{
    "config": {
      "target": "http://localhost:3000",
      "phases": [
        {
          "duration": "1",
          "arrivalRate": 1
        }
      ],
      "processor": "../../util/performance/processorArtillery.js",
      "payload": [
        {
          "path": "../../config/desarrollo.csv",
          "fields": [
            "x",
            "rut"
          ]
        }
      ]
    },
    "scenarios": [
      {
        "flow": [
          {
            "get": {
              "url": "/servicios/v1/personasnaturales/{{rut}}",
              "afterResponse": "procesaRespuestaPersonasNaturales"
            }
          }
        ]
      }
    ]
  }