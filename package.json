{
  "name": "pagoservicios",
  "version": "0.1.0",
  "artefacto": "servicio",
  "description": "Microservicio del dominio de pago de servicios en la cual se exponen operaciones de acceos a BUS de pago de servicios (massive)",
  "main": "index.js",
  "scripts": {
    "test": "npm run unitTesting && npm run checkCoverage",
    "start": "micro",
    "contrato": "mocha repository/**/*.contrato.js",
    "performance": "node ./util/performance/artillery.js && node ./util/performance/validaPerformance.js",
    "performanceProd": "node ./util/performance/artillery.js",
    "static": "./node_modules/.bin/eslint repository/**/*.controller.js",
    "fixstatic": "./node_modules/.bin/eslint repository/**/*.controller.js --fix",
    "dev": "micro-dev",
    "testlocal": "npm run test && npm run contrato && npm run performance",
    "unitTesting": "nyc --all --reporter=lcov --reporter=text mocha repository/**/*.spec.js",
    "checkCoverage": "nyc check-coverage --lines $COVERAGELINES --functions $COVERAGEFUNCTIONS --branches $COVERAGEBRANCHES"
  },
  "author": "CÃ©lula Cobre",
  "email": "celulacobre@kibernum.com",
  "contributors": [
    "Ulises Layera <ulayera@externos.bancoestado.cl>"
  ],
  "nyc": {
    "exclude": [
      "**/*.spec.js",
      "**/*.contrato.js",
      "api",
      "config",
      "repository/healthcheck/",
      "repository/notFound/",
      "index.js",
      "coverage",
      "util"
    ]
  },
  "license": "ISC",
  "dependencies": {
    "arquitecturadigital-bech": "^1.3.1",
    "child-process-promise": "^2.2.1",
    "fs-extra": "^5.0.0",
    "istanbul": "^0.4.5",
    "journal-bech": "^1.0.5",
    "jsonschema": "^1.2.4",
    "jwt-simple": "^0.5.1",
    "micro": "^9.1.4",
    "microrouter": "^3.1.1",
    "moment": "^2.22.0",
    "mysql": "^2.15.0",
    "promise-mysql": "^3.2.1",
    "redis-bech": "^1.0.3",
    "request": "^2.85.0",
    "request-promise": "^4.2.2",
    "request-promise-native": "^1.0.5",
    "rewiremock": "^3.7.2",
    "seguridad-bech": "^1.2.1",
    "soap-client-bech": "^1.1.0",
    "systeminformation": "^3.38.0",
    "uuid": "^3.2.1"
  },
  "devDependencies": {
    "babel-plugin-istanbul": "^4.1.6",
    "babel-register": "^6.26.0",
    "chai": "^4.1.2",
    "chai-http": "^3.0.0",
    "chai-as-promised": "^7.1.1",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.11.0",
    "expect-to-be-a-promise": "^1.0.2",
    "micro-dev": "^2.2.2",
    "mocha": "^5.0.5",
    "mock-express-response": "^0.2.2",
    "nyc": "^11.6.0",
    "proxyquire": "^2.0.1"
  }
}
